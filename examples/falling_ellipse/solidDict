    {
        name circle1;
        type Circle;
        radius 0.4;
    }
    shape2
    {
        name ellipse1;
        type Ellipse;
        radiusa 0.3;
        radiusb 0.15;
        com (0 0 0);
    }
}

materials
{
    material1
    {
      name mat1;
      type General;
      rho 2.0;
    }
}

motions
{
    motion1
    {
       name free1;
       type Motion111111;
    }
    motion2
    {
      name static1;
      type Motion000000;
    }
    motion3
    {
      name onlyzrot1;
      type Motion000001;
    }
